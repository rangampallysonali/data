pipeline {
    agent any
    stages {
        stage('install dependencies') {
            steps {
                sh '''
                python3 -m pip install --upgrade pip
                pip3 install -r requirements.txt
                '''
            }
        }
        stage('Run The code') {
            steps {
                sh '''
                
                python3 main.py
                '''
            }
        }
    }
}
